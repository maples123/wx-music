.search{
  background-color: #b3d4d8;
}
.search .search-form{
  display:block;
  padding:10rpx;
}
.search .search-form input{
  float:left;
  width:650rpx;
  height:70rpx;
  border-bottom:1px solid #fff;
  color:#fafafafa;
}
.search .search-form button{
  float:right;
  width:70rpx;
  height:70rpx;
  background:transparent;
}
.search .search-form button image{
  width:100%;
  height:100%;
}